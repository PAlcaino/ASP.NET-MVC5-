@model IEnumerable<GestionToxicologica.Models.Denuncia>

@{
    ViewBag.Title = "Index";
}

<h2>Denuncias</h2>

<p>
    @Html.ActionLink("Ingresar Denuncia", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id_Comuna)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Id_Estado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Ingreso)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Cierre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CorreoUsuario)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id_Comuna)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_Estado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Ingreso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Cierre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CorreoUsuario)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id_denuncia }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id_denuncia }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.Id_denuncia })
        </td>
    </tr>
}

</table>
