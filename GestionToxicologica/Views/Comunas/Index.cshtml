@model IEnumerable<GestionToxicologica.Models.Comuna>

@{
    ViewBag.Title = "Index";
}

<h2>Comunas</h2>

<p>
    @Html.ActionLink("Agrergar Comuna", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.Id_Comuna }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.Id_Comuna }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.Id_Comuna })
        </td>
    </tr>
}

</table>
